<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <com.example.henry.myapplication1.mycompnent.ClearEditText
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:background="@drawable/qq_edit_login"
            android:id="@+id/enterpriseSearchContain_startDate"/>

        <com.example.henry.myapplication1.mycompnent.ClearEditText
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:background="@drawable/qq_edit_login"
            android:id="@+id/enterpriseSearchContain_endDate"/>
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="查询"
            android:background="@drawable/background_button_div"
            android:id="@+id/enterpriseSearchContain_btn_date"/>
    </LinearLayout>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/enterpriseSearchContain_lv_date">
    </ListView>
</LinearLayout>